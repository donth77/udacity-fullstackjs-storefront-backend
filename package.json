{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "create-test-db": "dotenv cross-var db-migrate db:create %POSTGRES_DB_TEST%",
    "drop-test-db": "dotenv cross-var db-migrate db:drop %POSTGRES_DB_TEST%",
    "test": "yarn build && yarn drop-test-db && yarn create-test-db && db-migrate --env test up && cross-env ENV=test jasmine --config=jasmine.json --reporter=jasmine-console-reporter && db-migrate --env test down --count 4 && yarn drop-test-db",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc.json src/**/**.ts --write",
    "build": "npx tsc",
    "db-down-all": "db-migrate down --count 4"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "dot-env": "^0.0.1",
    "express": "^4.17.1",
    "jasmine-console-reporter": "^3.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.22",
    "pg": "^8.5.1",
    "supertest": "^6.3.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.1.0",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9"
  }
}
